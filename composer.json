{
    "name": "connysjoblom/advent",
    "description": "My Advent of Code Solutions.",
    "keywords": [
        "framework",
        "laravel",
        "laravel zero",
        "console",
        "cli"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Conny Sj√∂blom",
            "email": "conny@sjoblom.io"
        }
    ],
    "require": {
        "php": "^8.4.0",
        "ext-gmp": "*",
        "illuminate/http": "^12.29",
        "illuminate/translation": "^12.29",
        "illuminate/validation": "^12.29",
        "laravel-zero/framework": "^12.0"
    },
    "require-dev": {
        "larastan/larastan": "^3.7",
        "laravel/pint": "^1.25",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "advent"
    ]
}
